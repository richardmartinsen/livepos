{
  "name": "LivePos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "lint": "sass-lint && ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build",
    "build --prod": "npm run lint && ng build --prod --no-sourcemap --stats-json && npm run sw",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "static-serve": "cd dist && live-server --port=4300 --host=localhost --entry-file=/index.html",
    "deploy": "ng build -prod;npm run sw;firebase deploy",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.4.4",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/router": "4.4.4",
    "angular2-material-datepicker": "0.5.0",
    "angularfire2": "4.0.0-rc.1",
    "blueprint-css": "2.1.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "firebase": "4.0.0",
    "firebase-tools": "3.7.0",
    "font-awesome": "4.7.0",
    "fullcalendar": "3.4.0",
    "hammerjs": "2.0.8",
    "intl": "1.2.5",
    "md2": "0.0.25",
    "minx": "r-park/minx.git",
    "moment": "2.18.1",
    "primeng": "4.1.2",
    "promise-polyfill": "6.0.2",
    "rxjs": "5.1.0",
    "sass-loader": "~6.0.5",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.4",
    "@angular/compiler-cli": "4.4.4",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.53",
    "@types/node": "~6.0.60",
    "angular-ide": "0.9.22",
    "codelyzer": "~3.0.1",
    "firebase-tools": "3.6.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "~5.1.0",
    "sass-lint": "1.10.2",
    "sw-precache": "5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~5.0.0",
    "typescript": "~2.2.2",
    "webpack-bundle-analyzer": "2.3.1"
  }
}
